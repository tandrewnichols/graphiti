{
  "name": "graphiti",
  "description": "A neo4j ORM and graph modeler",
  "version": "1.0.0",
  "main": "lib/graphiti.js",
  "scripts": {
    "test": "npm run test:unit && npm run test:browser -- --single-run --browsers PhantomJS",
    "watch": "npm run test:unit -- --watch --reporter dot",
    "pretest": "eslint lib/**/*.js --config .eslint.json",
    "travis": "node scripts/matches v4 && npm run test:coverage || npm run test",
    "cover:unit": "istanbul cover --root ./lib --dir coverage/unit -x=scripts/** _mocha -- test/unit/**/*.js",
    "codeclimate": "codeclimate-test-reporter < coverage/merged/lcov.info",
    "test:coverage": "npm run cover:unit && npm run cover:browser && npm run merge && npm run codeclimate",
    "open:unit": "open coverage/unit/lcov-report/index.html",
    "test:unit": "mocha test/unit/**/*.js --ui bdd --require should",
    "test:browser": "karma start",
    "test:integration": "mocha test/integration/**/*.js --ui bdd --require should",
    "cover:browser": "NODE_ENV=test karma start ./karma.coverage.conf.js",
    "merge": "istanbul report --dir coverage/merged --include=coverage/**/coverage.json",
    "open:browser": "open coverage/browser/html/index.html",
    "open:merged": "open coverage/merged/lcov-report/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/tandrewnichols/graphiti.git"
  },
  "keywords": [
    "neo4j",
    "neo",
    "orm ",
    "graph",
    "graphdb",
    "database",
    "orm",
    "graffiti"
  ],
  "author": {
    "name": "Andrew Nichols",
    "email": "tandrewnichols@gmail.com",
    "url": "www.tandrewnichols.me"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tandrewnichols/graphiti/issues"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "homepage": "https://github.com/tandrewnichols/graphiti",
  "dependencies": {
    "async": "^2.1.5",
    "lodash": "^4.17.4",
    "request": "^2.79.0"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.22.0",
    "body-parser": "^1.16.1",
    "chalk": "^1.1.3",
    "codeclimate-test-reporter": "^0.4.1",
    "eslint": "^3.16.0",
    "eslint-codeframe-formatter": "^1.0.2",
    "express": "^4.14.1",
    "istanbul": "^0.4.5",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-webpack": "^2.0.2",
    "mocha": "^3.2.0",
    "opn": "^5.0.0",
    "proxyquire": "^1.7.11",
    "rimraf": "^2.6.0",
    "should": "^11.2.0",
    "should-sinon": "0.0.5",
    "sinon": "^2.0.0",
    "uglify-js": "^3.0.3",
    "webpack": "^2.2.1"
  }
}
